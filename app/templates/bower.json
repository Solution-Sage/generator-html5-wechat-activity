        <% } %>

        <% if(resetCss) { %>
        ,"HTML5-Reset": "html5-reset#~2.1.3"
        <% } %>

        <% if(animateCss) { %>
        ,"animate.css": "https://github.com/daneden/animate.css.git"
        <% } %>


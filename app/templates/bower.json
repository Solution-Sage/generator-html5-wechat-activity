{
    "name": "<%= projectName %>",
    "description": "",
    "main": "index.js",
    "authors": [
        "cs_victor <cs_victor@126.com>"
    ],
    "license": "ISC",
    "moduleType": [
        "amd"
    ],
    "homepage": "",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies": {

        <% if(useZeptoJs) {%>
        "zeptojs": "~1.1.6"
        <% } %>

        <% if(useJquery) {%>
        "jquery": "~2.1.4"
        <% } %>

        <% if(usePageSwitch) { %>
        ,"pageSwitch": "https://github.com/qiqiboy/pageSwitch.git"
        <% } %>

        <% if(useQjs) { %>
        ,"qjs": "https://github.com/kriskowal/q.git#1.4.1"
        <% } %>

        <% if(usePreload) {%>
        "html5Preloader.js": "https://github.com/codemonkey-victor/html5Preloader.js.git#cde87ce"
        <% } %>

        <% if(useNormalizeCss) { %>
        ,"normalize-css": "normalize.css#~3.0.3"
        <% } %>

        <% if(useResetCss) { %>
        ,"HTML5-Reset": "html5-reset#~2.1.3"
        <% } %>

        <% if(useAnimateCss) { %>
        ,"animate.css": "https://github.com/daneden/animate.css.git"
        <% } %>

    }
}

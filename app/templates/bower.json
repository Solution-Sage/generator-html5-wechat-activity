    "homepage": "",
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests"
    ],
    "dependencies": {

        "html5Preloader.js": "https://github.com/codemonkey-victor/html5Preloader.js.git#cde87ce"


